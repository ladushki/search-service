{
    "name": "francescomalatesta/laravel-api-boilerplate-jwt",
    "description": "An API Boilerplate to create a ready-to-use REST API in seconds.",
    "keywords": ["laravel", "api", "boilerplate", "jwt", "auth", "rest"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "0.11.*",
        "dingo/api": "^2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpspreadsheet": "^1.9",
        "tymon/jwt-auth": "1.0.*",
        "zachflower/eloquent-interactions": "^0.2.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.3.3",
        "phpspec/phpspec": "^5.1.1",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "2.*",
        "phploc/phploc": "^5.0.0",
        "sebastian/phpcpd": "^4.1.0",
        "sensiolabs/security-checker": "^5.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "resources/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan jwt:secret -f"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "phpunit": [
            "vendor/bin/phpunit --colors=always"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
